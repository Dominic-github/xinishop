model Shop {
  user_id       String     @id
  shop_name     String
  shop_password String
  shop_email    String     @unique
  shop_msisdn   String
  shop_status   ShopStatus @default(active)
  shop_verify   Boolean    @default(false)
  created_at    DateTime   @default(now())
  updated_at    DateTime   @updatedAt
}

enum ShopStatus {
  active
  inactive
  deleted
}
